            "body": {
              "mode": "raw",
              "raw": "{\n    \"id\": \"<long>\",\n    \"name\": \"<string>\",\n    \"description\": \"<string>\",\n    \"price\": \"<double>\",\n    \"quantity\": \"<integer>\",\n    \"stock\": \"<long>\"\n}",
